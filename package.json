{
  "name": "vwc-web-app",
  "version": "1.0.0",
  "repository": "Vets-Who-Code/vwc-site",
  "dependencies": {
    "@contentful/rich-text-react-renderer": "13.2.0",
    "babel-preset-gatsby": "0.2.0",
    "dotenv": "8.2.0",
    "gatsby": "2.11.7",
    "gatsby-background-image": "0.7.6",
    "gatsby-image": "2.2.3",
    "gatsby-link": "2.2.0",
    "gatsby-plugin-create-client-paths": "2.1.1",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-gtag": "^1.0.11",
    "gatsby-plugin-mailchimp": "https://github.com/benjaminhoffman/gatsby-plugin-mailchimp.git#gatsby-v2",
    "gatsby-plugin-prefetch-google-fonts": "1.4.2",
    "gatsby-plugin-react-helmet": "3.1.0",
    "gatsby-plugin-sharp": "2.2.1",
    "gatsby-source-contentful": "2.1.54",
    "gatsby-source-filesystem": "2.1.1",
    "gatsby-transformer-sharp": "2.2.0",
    "generate": "0.14.0",
    "intersection-observer": "0.7.0",
    "nuka-carousel": "4.5.9",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-modal": "3.8.2",
    "react-slick": "0.24.0",
    "react-toastify": "5.3.1",
    "reading-time": "1.2.0",
    "sharp": "0.22.1",
    "slick-carousel": "1.8.1",
    "typeface-lato": "0.0.54",
    "typeface-roboto-slab": "0.0.72",
    "typeface-source-sans-pro": "0.0.54"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format:src": "prettier --write \"src/**/*.js\"",
    "format:config": "prettier --write \"gatsby-*.js\"",
    "format": "npm run format:src && npm run format:config",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "view:coverage": "open coverage/lcov-report/index.html",
    "lint": "lint-staged",
    "heroku-postbuild": "gatsby build"
  },
  "devDependencies": {
    "@babel/core": "7.5.4",
    "@testing-library/react": "8.0.4",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-preset-gatsby-package": "0.2.0",
    "cross-env": "6.0.3",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "gatsby-plugin-purgecss": "4.0.0",
    "ghooks": "2.0.4",
    "jest": "24.8.0",
    "jest-dom": "3.5.0",
    "lint-staged": "9.0.0",
    "react-test-renderer": "16.8.6"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "npm run format",
      "git add"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint; npm run format; npm run test"
    }
  },
  "resolutions": {
    "sharp": "0.22.1"
  }
}
