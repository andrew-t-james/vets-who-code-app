{
  "name": "vwc-web-app",
  "version": "1.0.0",
  "repository": "Vets-Who-Code/vwc-site",
  "dependencies": {
    "@reach/dialog": "0.2.8",
    "gatsby": "2.4.3",
    "gatsby-image": "2.0.41",
    "gatsby-link": "2.1.1",
    "gatsby-plugin-create-client-paths": "2.0.5",
    "gatsby-plugin-mailchimp": "https://github.com/benjaminhoffman/gatsby-plugin-mailchimp.git#gatsby-v2",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sharp": "2.0.37",
    "gatsby-source-contentful": "2.0.56",
    "gatsby-source-filesystem": "2.0.33",
    "gatsby-transformer-sharp": "2.1.19",
    "generate": "0.14.0",
    "jest": "24.8.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-modal": "3.8.1",
    "react-slick": "0.24.0",
    "react-spring": "8.0.20",
    "react-toastify": "5.1.0",
    "sharp": "0.22.1",
    "slick-carousel": "1.8.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "view:coverage": "open coverage/lcov-report/index.html",
    "lint": "lint-staged",
    "heroku-postbuild": "gatsby build"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-preset-gatsby-package": "0.1.4",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-plugin-react": "7.13.0",
    "gatsby-plugin-purgecss": "3.1.1",
    "ghooks": "2.0.4",
    "jest-dom": "3.2.2",
    "lint-staged": "8.1.6",
    "prettier": "1.17.0",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "7.0.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "yarn format",
      "git add"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "yarn lint; yarn test"
    }
  }
}
