    "typeface-roboto-slab": "1.1.3",
    "typeface-source-sans-pro": "0.0.75"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "gen:icon": "node scripts/build-icon-json.js",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format:src": "prettier --write \"src/**/*.js\"",
    "format:config": "prettier --write \"gatsby-*.js\"",
    "format": "npm run format:src && npm run format:config",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "view:coverage": "open coverage/lcov-report/index.html",
    "lint": "lint-staged",
    "heroku-postbuild": "gatsby build"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@testing-library/react": "9.4.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-preset-gatsby-package": "0.2.16",
    "chalk": "3.0.0",
    "cross-env": "7.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "gatsby-plugin-purgecss": "4.0.1",
    "ghooks": "2.0.4",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "react-test-renderer": "16.12.0",
    "svgson": "4.0.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "npm run format",
      "git add"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint; npm run format; npm run test"
    }
  },
  "resolutions": {
    "sharp": "0.23.2"
  }
}
