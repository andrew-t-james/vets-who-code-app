    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-modal": "3.11.1",
    "react-slick": "0.25.2",
    "react-toastify": "5.5.0",
